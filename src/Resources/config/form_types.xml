<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="zentlix_block.block.create_form" class="Zentlix\BlockBundle\UI\Http\Web\Form\Block\CreateForm">
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="translator"/>
            <tag name="form.type"/>
        </service>
        <service id="zentlix_block.block.update_form" class="Zentlix\BlockBundle\UI\Http\Web\Form\Block\UpdateForm">
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="translator"/>
            <tag name="form.type"/>
        </service>
        <service id="zentlix_block.block.visual_edit_form" class="Zentlix\BlockBundle\UI\Http\Web\Form\Block\VisualEditForm">
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="translator"/>
            <tag name="form.type"/>
        </service>
    </services>
</container>